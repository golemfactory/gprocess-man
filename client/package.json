{
    "name": "gprocess-man-client",
    "version": "0.0.0",
    "description": "gprocess-man JS client",
    "private": true,
    "files": ["lib"],
    "main": "lib/index.js",
    "imports": {
        "#lib": "./src/index.ts"
    },
    "dependencies": {
        "ts-proto": "1.181"
    },
    "devDependencies": {
        "@types/node": "22.5",
        "rimraf": "6.0",
        "ts-node": "10.9",
        "typescript": "5.5"
    },
    "scripts": {
        "build": "tsc",
        "build:proto": "cp ../gprocess-proto/protos/gprocess.proto . && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/ --ts_proto_opt=oneof=unions ./gprocess.proto",
        "start": "node lib/example.js",
        "clean": "rimraf lib gprocess.proto"
    }
}
